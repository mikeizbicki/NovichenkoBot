ne"></div><div><div class="wildcard PageBuilder-page"><div class="PageBuilder-pageWrapper"><div class="PageBuilder-pageRow PageBuilder-containerFluidWidths"><div class="PageBuilder-col-12 PageBuilder-col"><div id="Wildcard-1"><div class="Wildcard-wrapper"><script src="https://code.jquery.com/jquery-2.1.4.min.js"></script>
<script type="text/javascript">
   $(function () {
       var imageArray = [
           "https://image.cnbcfm.com/api/v1/image/105217956-105086117-GettyImages-936957818.1910x1000.jpg?v=1526663420"
           , "https://image.cnbcfm.com/api/v1/image/105217957-104989962-RTX4QJW1.1910x1000.jpg?v=1526663482"
           , "https://image.cnbcfm.com/api/v1/image/105217959-104846378-GettyImages-109352836.1910x1000.jpg?v=1526663543"];
   
       var courtesyTextArray = [
           "Image courtesy of Getty Images",
           "Image courtesy of Reuters",
           "Image courtesy of Getty Images"];
   
       var randomIndex = Math.floor(Math.random() * imageArray.length);
       var randomizedImage = imageArray[randomIndex];
       var courtesyText = courtesyTextArray[randomIndex];
       $(".FullImage-img").css("backgroundImage", "url('" + randomizedImage + "')");
       $(".FullImage-imageEmbedCredit").html(courtesyText);
   
       $(window).scroll(function () {
   
           var imgTop = document.getElementsByClassName("FullImage-img")[0].getBoundingClientRect().top;
           var windowTop = window.innerHeight;
           var area = 500 + windowTop;
           var backgroundSizeValue = -0.25 * (100 * ((imgTop + 500) / area)) + 125;
           $(".FullImage-img").css("backgroundSize", backgroundSizeValue + "%")
       });
   });
  fetch('https://webql.cnbcfm.com/graphql', {
        method: 'POST',
        headers: {
            'Content-Type': 'application/json'
        },
        body: JSON.stringify({
            query: '{\n mostPopular(source: parsely, id: 100003114, tag: \"Articles\"){\n assets{\n url\n linkHeadline\n }\n }\n}'
        }),
    })
    .then(res => res.json())
    .then(res => {
      html = "";
      res.data.mostPopular.assets.map((asset, i) => (
        html += "<li class=\"TrendingNow-storyItem\"><div class=\"TrendingNow-number\">" + (i + 1)+ "</div><a href=\"" + asset.url + "\" class=\"TrendingNow-title\">" + asset.linkHeadline + "</a></li>"
      ))
    	document.getElementById("404-not-found-previews").innerHTML = html;
      document.getElementById("loading-indicator").style.display = "none";
    });
</script>
