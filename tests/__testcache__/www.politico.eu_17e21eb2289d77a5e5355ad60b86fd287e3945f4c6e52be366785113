window.onload = function(){
                                                                    
var countDownDate = new Date(1576188000000).getTime();
var x = setInterval(function() {
var now = new Date().getTime();
var distance = countDownDate - now;
var days = Math.floor(Math.max(distance / (1000 * 60 * 60 * 24),0));
var hours = Math.floor(Math.max((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60),0));
var minutes = Math.floor(Math.max(((distance % (1000 * 60 * 60)) / (1000 * 60)),0));
var seconds = Math.floor(Math.max((distance % (1000 * 60))/ 1000,0));
document.getElementById('pollofpollswidgetdays').innerHTML = days;
document.getElementById('pollofpollswidgethours').innerHTML = hours;
document.getElementById('pollofpollswidgetminutes').innerHTML = minutes;
document.getElementById('pollofpollswidgetseconds').innerHTML = seconds;

  if (distance < -3600000) {
    clearInterval(x);
  }
  }, 1000);
}

