    <script type="text/javascript">
        jQuery(document).ready(function ($) {
            cname = "digocookies";
            
            /* Revisar politica de cookies */
            var politica = getCookie(cname);
            if (politica === "") {
                /* Mostrar politica de cookies */
                $("#digo_cookies").slideDown('fast');

                /* Guardar cookie de politica de cookies */
                setCookie(cname, "aceptado", 365);
            }

            $("#cerrar_cookies").click(function () {
                $("#digo_cookies").slideUp('fast');
            });

        });

        function setCookie(cname, cvalue, exdays) {
            var d = new Date();
            d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
            var expires = "expires=" + d.toUTCString();
            document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
        }

        function getCookie(cname) {
            var name = cname + "=";
            var ca = document.cookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') {
                    c = c.substring(1);
                }
                if (c.indexOf(name) == 0) {
                    return c.substring(name.length, c.length);
                }
            }
            return "";
        }

    </script>
    
