<script type="text/javascript">
		var paragraphCount = 0;
	$('#article-text > p').each(function () {
		if($(this).text().length > 80) {
			paragraphCount += 1;
		}
	});

		var thingsToPlace = 0;
	if($('#in-article-photo').length > 0) {
		thingsToPlace += 1;
	}
	$('.in-article-video').each(function() {
		thingsToPlace += 1;
	});
	if($('#related_articles_ad_block').length > 0) {
		thingsToPlace += 1;
	}
	$('.in-article-interactive').each(function() {
		thingsToPlace += 1;
	});
	$('.in-article-element').each(function() {
		thingsToPlace += 1;
	});

		var positions = Math.max(thingsToPlace, 3);
	var currentPosition = 1;
	
			if($("#article-text > p:contains('<gallery>')").length > 0) {
		var gallery = $('#in-article-photo').detach();
		$("#article-text > p:contains('<gallery>')").replaceWith(gallery);
	}
		else if($('#in-article-photo').length > 0) {
		if(paragraphCount > 0) {
			var newPosition = Math.ceil(paragraphCount / positions);
			var truePosition = findParagraph(newPosition);
			var gallery = $('#in-article-photo').detach();
			$('#article-text > p:nth-of-type(' + truePosition + ')').after(gallery);
			currentPosition += 1;
		}
		else {
			$('#article-text').append($('#in-article-photo'));
		}
	}
		
			if($("#article-text > p:contains('<video>')").length > 0) {
				var videoList = $('#article-content').find('.in-article-video');
		
				var count = 1;
		$("#article-text > p:contains('<video>')").each(function() {
			if(count <= videoList.length) {
				var video = videoList.get(count-1);
				$(this).replaceWith(video);
				count++;
			}
			else {
								$(this).remove();
			}
		});
	}
		else {
		if(paragraphCount > 0) {
			$('.in-article-video').each(function() {
				var newPosition = Math.ceil((paragraphCount * currentPosition) / positions);
				var truePosition = findParagraph(newPosition);
				var video = $(this).detach();
				$('#article-text > p:nth-of-type(' + truePosition + ')').after(video);
				currentPosition += 1;
			});
		}
		else {
			$('.in-article-video').each(function() {
				$('#article-text').append($(this));
			});
		}
	}
	
		if($('#related_articles_ad_block').length > 0) {
				if($("#article-text > p:contains('<related>')").length > 0) {
			var related = $('#related_articles_ad_block').detach();
			$("#article-text > p:contains('<related>')").replaceWith(related);
		}
				else {
			if(paragraphCount > 0) {
				var newPosition = Math.ceil(paragraphCount * currentPosition / positions);
				var truePosition = findParagraph(newPosition);
				var related = $('#related_articles_ad_block').detach();
				$('#article-text > p:nth-of-type(' + truePosition + ')').after(related);
				currentPosition += 1;
			}
			else {
				$('#article-text').append($('#related_articles_ad_block'));
			}
		}
	}
		
			if($("#article-text > p:contains('<interactive>')").length > 0) {
				var interactiveList = $('#article-content').find('.in-article-interactive');
		
				var count = 1;
		$("#article-text > p:contains('<interactive>')").each(function() {
			if(count <= interactiveList.length) {
				var interactive = interactiveList.get(count-1);
				$(this).replaceWith(interactive);
				count++;
			}
			else {
								$(this).remove();
			}
		});
	}
		else {
		if(paragraphCount > 0) {
			$('.in-article-interactive').each(function() {
				var newPosition = Math.ceil((paragraphCount * currentPosition) / positions);
				var truePosition = findParagraph(newPosition);
				var interactive = $(this).detach();
				$('#article-text > p:nth-of-type(' + truePosition + ')').after(interactive);
				currentPosition += 1;
			});
		}
		else {
			$('.in-article-video').each(function() {
				$('#article-text').append($(this));
			});
		}
	}
	
			if($("#article-text > p:contains('<element>')").length > 0) {
				var elementList = $('#article-content').find('.in-article-element');
		
				var count = 1;
		$("#article-text > p:contains('<element>')").each(function() {
			if(count <= elementList.length) {
				var element = elementList.get(count-1);
				$(this).replaceWith(element);
				count++;
			}
			else {
								$(this).remove();
			}
		});
	}
		else {
		if(paragraphCount > 0) {
			$('.in-article-element').each(function() {
				var newPosition = Math.ceil((paragraphCount * currentPosition) / positions);
				var truePosition = findParagraph(newPosition);
				var mainColumnElement = $(this).detach();
				$('#article-text > p:nth-of-type(' + truePosition + ')').after(mainColumnElement);
				currentPosition += 1;
			});
		}
		else {
			$('.in-article-element').each(function() {
				$('#article-text').append($(this));
			});
		}
	}
	
		function findParagraph(target) {
		var goodCount = 0;
		var totalCount = 0;
		var finalCount;
		found = false;
		
		finalCount = 1;
		$('#article-text > p').each(function () {
			totalCount += 1;
			if($(this).text().length > 80) {
				goodCount += 1;
			}
			
			if(goodCount == target && !found) {
				found = true;
				finalCount = totalCount;
				return false;
			}
		});
		
		return finalCount;
	}
	
		galleryOpen = false;
	$('.stripes-image-gallery').on("click", ".clearing-featured-img", function() {
		location.hash = "gallery";
		galleryOpen = true;
	});
	$('.stripes-image-gallery').on("click", ".clearing-close", function() {
		galleryOpen = false;
		if(location.hash == "#gallery") {
			window.history.back();
		}
	});
	window.onhashchange = function() {
		if(location.hash != "#gallery" && galleryOpen) {
			$('.clearing-close').click();
		}
	}
</script>

			    <div class="small-16 columns ndn_embed" id="ndn-video-below-story" data-config-distributor-id="92012" data-config-width="100%" data-config-aspect-ratio="16:9"></div>
	    	    	    	    	    	
	    	    
	</section>

